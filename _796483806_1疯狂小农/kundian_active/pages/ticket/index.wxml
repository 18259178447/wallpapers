<view>
    <view class="main">
        <view class="mian_title">
            <view>{{active.title}}</view>
            <view>{{orderData.sign_up[0].name}} ({{orderData.sign_up[0].tel}})</view>
        </view>
        <view class="main_ewm">
            <view>票号: {{orderData.order_number}}</view>
            <image src="{{orderData.qrcode}}"></image>
        </view>
        <view class="base_info">
      时间: {{active.start_time}}
    </view>
        <view bindtap="intoMap" class="base_info">
      地点: {{active.address}}
    </view>
        <view bindtap="doCall" class="base_info">
      联系方式: {{active.phone}}
    </view>
    </view>
</view>
