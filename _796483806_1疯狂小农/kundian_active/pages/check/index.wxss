page {
    background: #f4f4f4;
    font-size: 28rpx;
}

.order-info,.user-info {
    width: 94%;
    margin-left: 3%;
    border-radius: 10rpx;
    background: white;
    margin-top: 20rpx;
}

.order-info .order-li,.user-info .user-li {
    width: 90%;
    margin-left: 5%;
    height: 94rpx;
    border-bottom: 1rpx solid #f4f4f4;
    line-height: 94rpx;
}

.check-btn {
    width: 94%;
    height: 94rpx;
    line-height: 94rpx;
    text-align: center;
    color: white;
    margin-left: 3%;
    border-radius: 10rpx;
    margin-top: 40rpx;
    background: #16ba63;
}