.video {
    width: 100%;
    height: 430rpx;
}

.video video {
    width: 100%;
    height: 100%;
}

.covers {
    position: relative;
    width: 100%;
    height: 430rpx;
}

.covers image:nth-child(1) {
    width: 100%;
    height: 100%;
}

.covers image:nth-child(2) {
    position: absolute;
    width: 120rpx;
    height: 120rpx;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}