@import "../../__wuBaseWxss__/1.wxss";

.title-wrapper {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    padding: 10rpx 30rpx;
    box-sizing: border-box;
    z-index: 99999;
}

.title {
    font-size: 34rpx;
    font-weight: 500;
    color: #000000;
    letter-spacing: 3rpx;
    width: 100%;
}

.route-wrapper {
    display: flex;
    align-items: center;
    color: #A5A5A5;
    position: absolute;
    left: 85%;
    margin-top: -2rpx;
}

.more {
    font-size: 30rpx;
    margin-right: 5rpx;
}

.route-wrapper .iconfont {
    font-size: 26rpx;
    height: 100%;
}