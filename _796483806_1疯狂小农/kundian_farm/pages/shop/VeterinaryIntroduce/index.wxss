.banner {
    width: 100%;
    height: 375rpx;
}

.banner image {
    width: 100%;
    height: 100%;
}

.content {
    width: 70%;
    padding-bottom: 10rpx;
}

.headImg {
    width: 30%;
    height: 672rpx;
    text-align: center;
}

.headImg image {
    width: 140rpx;
    height: 140rpx;
    border-radius: 50%;
    border: 30rpx solid #fc8535;
}

.veterinaryName {
    width: 300rpx;
    text-align: right;
    border-bottom: 3rpx solid #9f9fa0;
}

.veterinaryNames {
    width: 300rpx;
    text-align: left;
    border-bottom: 3rpx solid #9f9fa0;
}

.certificateImg {
    width: 230rpx;
    height: 200rpx;
    float: left;
    margin-left: 10rpx;
    margin-top: 10rpx;
}

.certificateImg image {
    width: 100%;
    height: 100%;
}

.en-title {
    width: 90%;
    margin-left: 5%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.vet-con {
    width: 100%;
    height: auto;
    font-size: 30rpx;
    padding-bottom: 40rpx;
    margin-top: 20rpx;
}

.vet-con .vet-ul {
    width: 94%;
    margin-left: 3%;
    height: auto;
    overflow: hidden;
}

.vet-con .vet-ul .left {
    width: 75%;
    float: left;
    height: auto;
    text-align: right;
}

.vet-con .vet-ul .right {
    width: 25%;
    height: auto;
    float: left;
}

.vet-con .vet-ul .right .head {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    background: #fc8535;
    text-align: center;
    position: relative;
}

.vet-con .vet-ul .right .head image {
    width: 80%;
    height: 80%;
    border-radius: 50%;
    margin-top: 10%;
}

.vet-con .vet-ul .left .name {
    width: 95%;
    height: 60rpx;
    line-height: 60rpx;
    text-align: right;
}

.vet-con .vet-ul .left .vet-con-content {
    width: 95%;
    text-align: center;
    padding-top: 20rpx;
    color: gray;
    letter-spacing: 5rpx;
    font-size: 26rpx;
}

.vet-con .vet-ul .left .img {
    width: 95%;
    height: auto;
    padding-top: 10rpx;
    padding-bottom: 20rpx;
    overflow: hidden;
}

.vet-con .vet-ul .left .img image {
    width: 150rpx;
    height: 150rpx;
    margin-right: 10rpx;
    margin-top: 20rpx;
    float: left;
    box-shadow: 7rpx 5rpx 20rpx gray;
}

.vet-con .vet-ul .line {
    width: 95%;
    height: 5rpx;
    background: linear-gradient(to right,white,gray);
}

