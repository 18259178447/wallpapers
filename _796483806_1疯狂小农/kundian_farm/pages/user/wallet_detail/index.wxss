page {
    background: #f4f4f4;
}

.con {
    background: white;
}

.con .ul {
    width: 100%;
    height: 100rpx;
    border-bottom: 1rpx solid #f4f4f4;
    padding: 10px 0 10rpx 0;
}

.ul-top,.ul-btm {
    width: 94%;
    margin-left: 3%;
    height: 50rpx;
    line-height: 50rpx;
}

.ul-top .title {
    font-size: 30rpx;
}

.ul-top .time {
    font-size: 26rpx;
    color: gray;
    float: right;
}

.ul-btm .yue {
    font-size: 24rpx;
}

.ul-btm .do-type {
    font-size: 30rpx;
    float: right;
}