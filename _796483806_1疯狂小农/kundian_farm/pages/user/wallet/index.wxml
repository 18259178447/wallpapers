<view class="back" style="background: linear-gradient(to right, {{farmSetData.assist_color}}, {{farmSetData.background_color}});">
    <text class="yue-title">余额账户(元)</text>
    <view class="yue-val">{{userInfo.money}}</view>
</view>
<view bindtap="intoCash" class="func bb">
    <text class="iconfont icon-youhuiquan1 f44" style="color:{{farmSetData.background_color}}"></text>
    <text>提现</text>
    <text class="iconfont icon-right right-icon"></text>
</view>
<view bindtap="intoRecord" class="func bb">
    <text class="iconfont icon-youhuiquan1 f44" style="color:{{farmSetData.background_color}}"></text>
    <text>提现记录</text>
    <text class="iconfont icon-right right-icon"></text>
</view>
<view bindtap="intoDetail" class="func">
    <text class="iconfont icon-dingdan2 f44" style="color:{{farmSetData.background_color}}"></text>
    <text>明细</text>
    <text class="iconfont icon-right right-icon"></text>
</view>
