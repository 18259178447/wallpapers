page {
    font-size: 30rpx;
}

.content {
    background: white;
    padding: 10rpx 0 20rpx 0;
    overflow: hidden;
}

.ul {
    width: 44%;
    height: 250rpx;
    border: 1rpx solid #16ba63;
    margin-left: 4%;
    border-radius: 10rpx;
    float: left;
    margin-top: 20rpx;
}

.ul .ul-title,.ul .ul-status {
    width: 90%;
    height: 100rpx;
    text-align: center;
    padding: 20rpx 10rpx;
    margin-left: 5%;
}