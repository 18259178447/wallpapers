page {
    background: #f4f4f4;
    font-size: 30rpx;
}

.content {
    background: white;
    padding: 10rpx 0 20rpx 0;
    overflow: hidden;
}

.ul {
    width: 44%;
    height: 340rpx;
    border: 1rpx solid #16ba63;
    margin-left: 4%;
    border-radius: 10rpx;
    float: left;
    margin-top: 20rpx;
}

.ul .ul-top-title {
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    background: #16ba63;
    color: white;
}

.ul .ul-top-value {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
}

.ul .ul-top-value-to {
    height: 210rpx;
}

.ul .ul-name {
    width: 100%;
    height: 80rpx;
    text-align: center;
    border-top: 1rpx solid #16ba63;
    line-height: 80rpx;
    color: gray;
}