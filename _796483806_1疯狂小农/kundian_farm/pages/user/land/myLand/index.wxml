<view wx:if="{{plate==1}}">
    <view class="container">
        <view class="flexSa mt30">
            <view>
                <view class="title_name" style="color:{{farmSetData.background_color}}">{{landDetail.count}}㎡</view>
                <view class="title_desc">当前面积</view>
            </view>
            <view>
                <view class="title_name" style="color:{{farmSetData.background_color}}">{{landDetail.exprie_time}}</view>
                <view class="title_desc">到期时间</view>
            </view>
        </view>
        <view class="orderInfo_container" style="border-top:1rpx dashed {{farmSetData.background_color}};border-bottom:1rpx dashed {{farmSetData.background_color}}">
            <view class="flex mt30">
                <view class="head">订单状态</view>
                <view>已支付</view>
            </view>
            <view class="flex mt30">
                <view class="head">订单号</view>
                <view>{{landDetail.order_number}}</view>
            </view>
            <view class="flex mt30">
                <view class="head">土地名称</view>
                <view>{{landData.land_name}}</view>
            </view>
            <view class="flex mt30">
                <view class="head">土地编号</view>
                <view>{{landSpec.land_num}}</view>
            </view>
            <view class="flex mt30">
                <view class="head">联系人</view>
                <view>{{landDetail.username}}</view>
            </view>
            <view class="flex mt30">
                <view class="head">联系号码</view>
                <view>{{landDetail.phone}}</view>
            </view>
        </view>
        <view class="seed_title" style="border-left:10rpx solid {{farmSetData.background_color}}">种植列表</view>
        <view class="seed_container" wx:for="{{seedData}}" wx:key="id">
            <view class="seed-info flex">
                <view class="info-title">种植名称</view>
                <view class="info-value">{{item.send_name}}</view>
            </view>
            <view class="seed-info flex">
                <view class="info-title">种植面积</view>
                <view class="info-value">{{item.count}} ㎡</view>
            </view>
            <view class="seed-info flex">
                <view class="info-title">播种时间</view>
                <view class="info-value">{{item.seed_time}}</view>
            </view>
            <view class="seed-info flex">
                <view class="info-title">种植天数</view>
                <view class="info-value">{{item.seedDay}}天</view>
            </view>
            <view class="seed-info flex">
                <view class="info-title">当前状态</view>
                <view class="info-value">{{item.status_txt}}</view>
            </view>
            <view class="seed-info flex">
                <view class="info-title">预计成熟时间</view>
                <view class="info-value">{{item.expect_time}}</view>
            </view>
            <view class="seed-btn flex">
                <view bindtap="changeSeedStstua" class="seed-status" data-seedid="{{item.id}}" data-status="1" data-statustxt="已播种" style="color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}" wx:if="{{item.status==0}}">播种</view>
                <view bindtap="changeSeedStstua" class="seed-status" data-seedid="{{item.id}}" data-status="2" data-statustxt="已成熟" style="color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}" wx:if="{{item.status==1}}">已成熟</view>
                <view bindtap="seedPick" class="seed-status" data-seedid="{{item.id}}" data-status="{{item.status}}" data-statustxt="{{item.status_txt}}" style="color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}" wx:if="{{item.status==5}}">已摘取</view>
                <view bindtap="sendTemplateToUser" class="seed-status" data-seedid="{{item.id}}" data-statustxt="{{item.status_txt}}" style="color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}">通知</view>
                <view bindtap="releases" class="seed-status" data-lid="{{item.lid}}" data-seedid="{{item.id}}" style="color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}">
                    <text class="iconfont icon-add3 f30" style="color:{{farmSetData.background_color}}"></text>状态跟踪</view>
            </view>
        </view>
    </view>
    <view class="block20"></view>
</view>
<view wx:if="{{plate==2}}">
    <view class="container">
        <view class="flexSa mt30">
            <view>
                <view class="title_name" style="color:{{farmSetData.background_color}}">{{animalOrder.count}}</view>
                <view class="title_desc">当前数量</view>
            </view>
            <view>
                <view class="title_name" style="color:{{farmSetData.background_color}}">{{animalOrder.create_time}}</view>
                <view class="title_desc">认养时间</view>
            </view>
        </view>
        <view class="orderInfo_container" style="border-top:1rpx dashed {{farmSetData.background_color}};border-bottom:1rpx dashed {{farmSetData.background_color}}">
            <view class="flex mt30">
                <view class="head">订单状态</view>
                <view>已支付</view>
            </view>
            <view class="flex mt30">
                <view class="head">订单号</view>
                <view>{{animalOrder.order_number}}</view>
            </view>
            <view class="flex mt30">
                <view class="head">商户订单号</view>
                <view>{{animalOrder.uniontid}}</view>
            </view>
            <view class="flex mt30">
                <view class="head">联系人</view>
                <view>{{animalOrder.username}}</view>
            </view>
            <view class="flex mt30">
                <view class="head">联系号码</view>
                <view>{{animalOrder.phone}}</view>
            </view>
        </view>
        <view class="seed_container">
            <view class="seed_title">认养列表</view>
            <view class="seed_container" wx:for="{{adoptData}}" wx:key="id">
                <view class="seed-info flex">
                    <view class="info-title">认养名称</view>
                    <view class="info-value">{{item.animalData.animal_name}}</view>
                </view>
                <view class="seed-info flex">
                    <view class="info-title">认养价格</view>
                    <view class="info-value">{{item.animalData.price}}</view>
                </view>
                <view class="seed-info flex">
                    <view class="info-title">认养时间</view>
                    <view class="info-value">{{item.adopt_time}}</view>
                </view>
                <view class="seed-info flex">
                    <view class="info-title">认养天数</view>
                    <view class="info-value">{{item.adopt_day}}天</view>
                </view>
                <view class="seed-info flex">
                    <view class="info-title">当前状态</view>
                    <view class="info-value">{{item.status_txt}}</view>
                </view>
                <view class="seed-info flex">
                    <view class="info-title">认养编号</view>
                    <view class="info-value">
                        <input bindblur="updateAdoptNumber" data-adoptid="{{item.id}}" name="adopt_number" value="{{item.adopt_number}}"></input>
                    </view>
                </view>
                <view class="seed-info flex">
                    <view class="info-title">预计成熟时间</view>
                    <view class="info-value">{{item.predict_ripe}}</view>
                </view>
                <view class="seed-btn flex">
                    <view bindtap="changeAdoptStstua" class="seed-status" data-adoptid="{{item.id}}" data-status="2" data-statustxt="认养中" style="color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}" wx:if="{{item.status==1||item.status==3}}">认养中</view>
                    <view bindtap="changeAdoptStstua" class="seed-status" data-adoptid="{{item.id}}" data-status="4" data-statustxt="已成熟" style="color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}" wx:if="{{item.status==2}}">已成熟</view>
                    <view bindtap="changeAdoptStstua" class="seed-status" data-adoptid="{{item.id}}" data-status="3" data-statustxt="已死亡" style="color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}" wx:if="{{item.status==2}}">已死亡</view>
                    <view bindtap="sendAdoptTemplateToUser" class="seed-status" data-adoptid="{{item.id}}" data-statustxt="{{item.status_txt}}" style="color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}">通知</view>
                    <view bindtap="releases" class="seed-status" data-adoptid="{{item.id}}" style="color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}">
                        <text class="iconfont icon-add3 f30" style="color:{{farmSetData.background_color}}"></text>
                    状态跟踪
                    </view>
                </view>
            </view>
        </view>
    </view>
    <view class="block20"></view>
</view>
<view bindtap="seedMature" class="output-mask" wx:if="{{is_show_estimate_dialog}}"></view>
<view class="output-info" wx:if="{{is_show_estimate_dialog}}">
    <view class="output-title">
        <text>产量预估</text>
        <text bindtap="seedMature" class="iconfont icon-delete1"></text>
    </view>
    <form bindsubmit="seedEstimate" data-status="2">
        <view class="info-ul flex">
            <view class="info-ul-title">收获重量</view>
            <view class="info-ul-value">
                <input name="weight" placeholder="收获重量"></input>
            </view>
            <view class="unit">kg</view>
        </view>
        <view class="info-ul flex">
            <view class="info-ul-title">售出单价</view>
            <view class="info-ul-value">
                <input name="sale_price" placeholder="元"></input>
            </view>
            <view class="unit">元</view>
        </view>
        <view class="ouput-btn" style="background:{{farmSetData.background_color}}">
            <button formType="submit">保存</button>
        </view>
    </form>
</view>
<view bindtap="seedPick" class="output-mask" wx:if="{{is_show_sure_dialog}}"></view>
<view class="output-info" wx:if="{{is_show_sure_dialog}}">
    <view class="output-title">
        <text>产量确认</text>
        <text bindtap="seedPick" class="iconfont icon-delete1"></text>
    </view>
    <form bindsubmit="gainSeed" data-status="3">
        <view class="info-ul flex">
            <view class="info-ul-title">收获重量</view>
            <view class="info-ul-value">
                <input name="weight" placeholder="kg"></input>
            </view>
            <view class="unit">kg</view>
        </view>
        <view class="info-ul flex">
            <view class="info-ul-title">售出单价</view>
            <view class="info-ul-value">
                <input name="sale_price" placeholder="元"></input>
            </view>
            <view class="unit">元</view>
        </view>
        <view class="ouput-btn">
            <button formType="submit">保存</button>
        </view>
    </form>
</view>
