/* pages1/live/live.wxss */
@import "/utils/base.wxss";
.select-box{
  padding-left: 52rpx;
}
.select-item{
  padding-top: 86rpx;
}
.select-item__bg{
  width: 662rpx;
  height: 136rpx;
}

.select-item__body{
  height: 136rpx;
  margin-top: -136rpx;
  line-height: 136rpx;
  padding-left: 70rpx;
}

.selet-item__left{
  width: 30rpx;
  height: 31rpx;
  background: 0 0 no-repeat;
  background-size: cover;
}
.selet-item__title{
  font-size: 37rpx;
  color: #61a397;
  margin-bottom: 10rpx;
}
.select-item__explain{
  white-space: pre-wrap;
  font-size: 34rpx;
  padding-left: 10rpx;
  color: #fff;
}
.select-item__explain_pl50{
  padding-left: 50rpx;
}

.selet-item__circle{
  background-image: url(data:image/png;base64,)
}

.selet-item__circle-on{
  background-image: url(data:image/png;base64,)
}

.select-item__delivery-warm{
  width: 63rpx;
  height: 63rpx;

}
.select-item__explain text{
  color: #f5cf87;
}


.address{
  width: 598rpx;
  background: #fff;
  border-radius: 8rpx;
  padding: 12rpx 20rpx;
  color: #4f4f4f;
  font-size: 28rpx;
  margin: 0 auto;
  box-sizing: border-box;
  line-height: 38rpx;
  margin-top: 24rpx;
}

.address-tip{
  font-size: 28rpx;
  text-align: center;
  color: #f5cf87;
  line-height: 50rpx;
}


.client__icon{
  width: 79rpx;
  height: 66rpx;

}

.client__text{
  font-size: 38rpx;
  color: #fff;
  margin-left: 10rpx;
}

.save-adress{
  color: #f5cf87;
  font-size: 28rpx;
  width: 194rpx;
  height: 54rpx;
  background: #c75c5c;
}