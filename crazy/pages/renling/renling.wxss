.banner-bg {
  width: 739rpx;
  height: 713rpx;
  margin: 0 auto;
}

.select-btn__circle-outer {
  width: 34rpx;
  height: 34rpx;
  border: 2rpx solid #414141;
  border-radius: 50%;
  box-sizing: border-box;
  padding: 6rpx 0 0 12rpx;
}

.select-btn__circle-inner {
  width: 14rpx;
  height: 14rpx;
  background-color: #414141;
  border-radius: 50%;
}
.area-select {
  margin-top: 50rpx;
}


.land-select__area-name {
  width: 161rpx;
  height: 45rpx;
  margin-left: 20rpx;
}

.land-select__item_b {
  margin-left: 98rpx;
}


.area-select__item{
  margin: 0 20rpx 30rpx;
}
.area-select__image{
  width: 192rpx;
  height: 80rpx;
}
.area-select__val{
  font-size: 36rpx;
  font-family: "nomal";
  margin-left: 10rpx;
  color: #414141;
}

