  padding-left: 60rpx;
  background: 12rpx center no-repeat;
  background-size: 32rpx;
  font-family: "nomal";
  font-size: 26rpx;
  color: #414141;
}

.rent-time{
  background-image: url(data:image/png;base64,);
  border-top: 2rpx solid #acd6c7;
}

.other__pointer{
  width: 14rpx;
  height: 29rpx;
