  background-size: cover;
  margin: 0 28rpx;
}

.other__name_gray{
  color: #9d9b9b;
}
.tel{
  background-image: url(data:image/png;base64,);
  text-align: left
}

.change-tel{
  color: #598718;
  margin-right: 60rpx;
}

.xieyi{
  margin-left: 56rpx;
  font-family: "nomal";
  padding-left: 60rpx;
  font-size: 16rpx;
  color: #414141;
  line-height: 60rpx;
    background:url(data:image/png;base64,) 12rpx center no-repeat;
  background-size: 29rpx 25rpx;
}

.xieyi text{
  color: #598718;
