}


.text-img{
  padding: 56rpx;
}
.text-img__item{
  width: 100%;
  height: auto;
}


.pay-nav{
  position: fixed;
  left: 0;
  right: 0;
  height: 88rpx;
  bottom: 122rpx;
  /* bottom: 0; */
  background-color: #fff;
  font-size: 50rpx;
  box-shadow: 0 -2rpx 50rpx rgba(0, 0, 0, 0.1)
}
.pay-nav__pirce{
  color: #338265;
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  margin-left: 60rpx;
}
.pay-nav__pirce text{
  font-size: 30rpx;
  color: #87c8b0;
}

.pay-nav__btn{
  background-color: #87c8b0;
  width: 244rpx;
  text-align: center;
  line-height: 88rpx;
