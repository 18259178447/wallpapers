page {
    background-color: #f5f5f5;
}

.success-nav {
    background: #000;
}

.nav-item-box {
    padding: 0;
    width: 100%;
}

.nav-item {
    margin: 0;
    width: 50%;
    text-align: center;
}

.nav-item.cur {
    border-bottom: 4rpx solid #000;
    color: #000;
}

.flex_start {
    display: flex;
    align-items: center;
}

.type_list {
    padding: 0 20rpx;
    width: 772rpx;
    box-sizing: border-box;
    flex-wrap: wrap;
}

.type_wrap:first-child {
    margin-left: 0;
}

.type_wrap:nth-child(5) {
    margin-left: 0;
}

.type_wrap {
    margin-top: 20rpx;
    margin-left: 20rpx;
    width: 162rpx;
    height: 60rpx;
    background-color: #fff;
    border-radius: 10rpx;
    box-shadow: 0 1px 3px rgba(223,223,223,.5);
    text-align: center;
}

.type_wrap text {
    font-size: 14px;
    color: #6F7785;
    line-height: 60rpx;
}

.ins-list-li {
    box-shadow: 0 1px 3px rgba(223,223,223,.5);
}

.ins-list-img {
    height: 212rpx;
}

.ins-list-author {
    height: 60rpx;
}

.ins-list-author .ins-list-name {
    max-width: 310rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}