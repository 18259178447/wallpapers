/* components/header/search/search.wxss */
@import "../../../utils/base.wxss";

.search{
  height: 74rpx;
  display: flex;
  align-items: center;
}
.search__share-btn{
  width: 74rpx;
  height: 74rpx;
  background: url(data:image/svg+xml;base64,) 20rpx center no-repeat;
  background-size: 48rpx;
  margin-right: 20rpx
}
.search__search-btn{
   height: 64rpx;
   background:#3a3a46;
   flex: 1;
   color: #a7a7b3;
   font-size:28rpx;
  border-radius: 4rpx;
  line-height: 68rpx;
  padding: 0 20rpx;
  position: relative;
}

.search__search-icon{
  width: 32rpx;
  height: 32rpx;
  background: url(data:image/svg+xml;base64,) center center no-repeat;
  background-size: cover;
  margin-right: 10rpx;
}