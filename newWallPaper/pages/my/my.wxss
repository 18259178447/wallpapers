/* pages/my/my.wxss */
.my-title{
  height: 74rpx;
  text-align: center;
  line-height: 74rpx;
  color: #a7a7b3;
  font-size: 36rpx;
  font-weight: bold;
}

.my-btns{
  margin-top: 20rpx;
}

.my-btns__item{
  height: 102rpx;
  background: #3a3a46;
  color: #a7a7b3;
  text-align: left;
  font-size: 28rpx;
  line-height: 102rpx;
  padding-left: 104rpx;
  position: relative;
}
.my-btns__item::before{
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  content: "";
  margin-left: 20rpx;
  border-bottom: 1rpx solid #a7a7b3
}

.my-btns__item_feedback{
  background: url(data:image/svg+xml;base64,) 33rpx center no-repeat;
  background-size: 48rpx;
}

.my-btns__item_contact{
  background: url(data:image/svg+xml;base64,) 27.5rpx center no-repeat;
  background-size: 64rpx;
}