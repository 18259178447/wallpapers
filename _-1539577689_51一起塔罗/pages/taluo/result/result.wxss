.imgs {
    position: fixed;
    top: 0;
    background: rgba(0,0,0,.3);
    width: 100%;
    bottom: 0;
    z-index: 999;
}

.imgs .img {
    width: 470rpx;
    position: absolute;
    top: 40%;
    height: 1096rpx;
    transform: translateY(-50%);
    left: 100%;
    transform-origin: left bottom;
    animation: move 1.5s forwards ease-out 1s;
    overflow: hidden;
}

.img scroll-view {
    width: 470rpx;
    height: 1096rpx;
}

.img image {
    width: 470rpx;
    position: absolute;
    top: 0;
}

.ibottom {
    position: fixed;
    display: flex;
    justify-content: space-around;
    left: 50%;
    transform: translateX(-50%);
    bottom: -1rpx;
    padding: 55rpx 0;
    background: #20163b;
    width: 470rpx;
}

.ibottom>button {
    width: 40%;
    font-size: 30rpx;
    background: #140b26;
    color: white;
    animation: big 0.5s infinite;
}

@-webkit-keyframes move {
    0% {
        top: 50%;
        transform: translate(0,-50%) rotate(-5deg);
        -webkit-transform: rotate(-10deg);
        transform: rotate(-10deg);
    }

    50% {
        transform: translate(-50%,-50%) rotate(-30deg);
        top: 50%;
        left: 50%;
    }

    100% {
        transform: translate(-50%,-50%) rotate(0deg);
        left: 50%;
        top: 50%;
    }
}

@keyframes move {
    0% {
        top: 50%;
        transform: translate(0,-50%) rotate(-5deg);
        -webkit-transform: rotate(-10deg);
        transform: rotate(-10deg);
    }

    50% {
        transform: translate(-50%,-50%) rotate(-30deg);
        top: 50%;
        left: 50%;
    }

    100% {
        transform: translate(-50%,-50%) rotate(0deg);
        left: 50%;
        top: 50%;
    }
}

@-webkit-keyframes big {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes big {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(1);
    }
}

.fu image {
    width: 30rpx;
}