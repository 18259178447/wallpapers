page {
    height: 100%;
    background: #fff;
}

.container {
    height: 100%;
    flex-direction: column;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.container,.input-box {
    display: flex;
    align-items: center;
}

.input-box {
    width: 658rpx;
    height: 94rpx;
    border-radius: 47rpx;
