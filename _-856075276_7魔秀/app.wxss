.match-container .reload-icon {
    width: 80rpx;
    height: 80rpx;
    animation: reload 1s linear;
    animation-iteration-count: infinite;
}

@-webkit-keyframes reload {
    0% {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(1turn);
    }
}

@keyframes reload {
    0% {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(1turn);
    }
}

