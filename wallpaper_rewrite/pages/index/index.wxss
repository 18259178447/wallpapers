
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
/* 轮播 */
.swiper{
  height: 405rpx;
  overflow: visible
}
.swiper swiper-item{
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: visible
}
.swiper navigator{
  width: 100%;
  height: 345rpx;
  /* height: 100%; */
  transform: scale(0.9246,0.9246) translateZ(0);
  transition: transform 0.5s;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 5rpx 5rpx 25rpx 0 rgba(0,0,0,0.1);
}
.swiper navigator.ori{
  transform: scale(1,1) translateZ(0);
}
.swiper-image{
  width: 100%;height: 100%;display: block;
}
