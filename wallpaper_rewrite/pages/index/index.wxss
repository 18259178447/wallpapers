/* 热门专辑 */
.hot-album,.hot-dynamic{
  padding:34rpx 16rpx 0 16rpx;
  margin-bottom: 20rpx;
  overflow: hidden;
  position: relative
}
.hot-album .album-view{
  display: flex;
  justify-content:space-between;
  margin-top: 34rpx;
}
.hot-album .album-view text{
  position: absolute;
  width: 64rpx;
  height: 34rpx;
  border-top-left-radius: 13rpx;
  border-bottom-left-radius: 13rpx;
  background-color: #239EFE;
  right: 0rpx;
  top: 10rpx;
  font-size: 20rpx;
  color: #FFFFFF;
  line-height: 34rpx;
}
.hot-album .album-view view{
  font-size: 28rpx;
  color: #323232;
  line-height: 88rpx;
  width: 354rpx;
  text-align: left;
  font-weight: 500
}
.hot-album .album-view text.act{
  background-color: #F49241;
}
.hot-album .album-view image{
  width: 354rpx;height: 188rpx;
  display:block;
  border-radius: 18rpx;
}



/* 热门状态 */
.dynamic-view{
  margin-top: 35rpx;
}
.dynamic-view-body{
  display: flex;
  padding-bottom: 50rpx;
}
.dynamic-view .left{
  width: 216rpx;
  height: 288rpx;
  display: block;
  margin-right: 50rpx;
}
.dynamic-view .right{
  width: 418rpx;
  position: relative
}

.dynamic-view .userInfo{
  display: flex;
  height: 80rpx;
  align-items: center;
  font-size: 28rpx;
  color: #4C4C4C;
  font-weight: 500
}

.dynamic-view .userInfo image{
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
  border-radius: 50%;
  background-image: url('https://minibizhi.313515.com/svg/default.svg');
  background-size: cover;
}
.dynamic-view .reply-word{
  font-size: 30rpx;
  color: #949494;
  line-height: 48rpx;
  text-align: left;
  /* -webkit-line-clamp: 3; */
  margin-top: 20rpx;
  height: 96rpx;
}
.hot-dynamic .module-title{background: none;}
.hot-dynamic .topicLabel{
  position: absolute;
  right: -16rpx;
  /* width: 80rpx; */
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #FFFFFF;
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  background: #239EFE;
  text-align: center;
  padding:0 14rpx;
}
.hot-dynamic .topicLabel.act{
 background: #F5A623;
}
.hot-dynamic .topicLabel.brandAct{
  background: #FF6D7F;
}
.hot-dynamic .toTopicBtn{
  position: absolute;
  bottom: 0;
  left: 0;
  max-width: 418rpx;
}

.activity-view{
  position: relative;
  margin-top: 36rpx;
}
.activity-view .bg{
  width: 718rpx;
  height: 380rpx;
  display: block;
  margin: 0 auto;
}
.activity-view text{
  position:absolute;
  left:0;
  top:50%;
  width:100%;
  transform:translateY(-50%);
  text-shadow:0 2rpx 6rpx rgba(0,0,0,0.29);
  font-weight:bold;
  text-align:center;
  box-sizing:border-box;
  padding:0 30rpx;
  color:#fff;
}
.activity-view .brand{
  display: flex;
  position: absolute;
  top: 26rpx;
  left: 30rpx;
  align-items: center;
}
.activity-view .brand image{
  display: block;
  width: 48rpx;
  height: 48rpx;
  border-radius: 13rpx;
  
}
.activity-view .brand image:nth-child(2){
  width: 14rpx;
  height: 14rpx;
  border-radius: 0;
  margin:0 12rpx;
}
/* 动态视频 */
