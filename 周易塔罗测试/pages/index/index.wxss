.day-list-box{
padding: 0 12px;
margin-top: 20rpx;
}
.day-list-item{
  font-size: 32rpx;
  line-height: 58rpx;
  color: #ff7585;
  text-decoration: underline
}
.page-section-title {
    padding: 0;
}

.section {
    background: #fff;
}

.diy-content-space {
    margin-top: 4px;
}

.diy-content-padded {
    margin: 0 12px;
    margin-bottom: 0;
    padding-bottom: 6px;
    border-bottom: 1px solid #e6e6e6;
    font-size: 14px;
    font-weight: 400;
    color: #8f8f94;
    line-height: 1;
    text-align: left;
    padding-top: 10px;
}

.swiper-item {
    display: block;
    width: 100%;
}

.page-section-title {
    margin-top: 60rpx;
    position: relative;
}

.info {
    position: absolute;
    right: 0;
    color: #353535;
    font-size: 30rpx;
}

.page-foot {
    margin-top: 50rpx;
}

.flex-wrp {
    display: flex;
    flex-wrap: wrap;
}

.flex-item {
    width: 25%;
    text-align: center;
    padding: 5px 8px;
    display: inline-block;
    box-sizing: border-box;
    position: relative;
}

.flex-item image {
    width: 60px;
    height: 60px;
    border-radius: 10px;
}

.imgae-title {
    font-size: 14px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.listitem-box {
    position: relative;
    width: 100%;
    height: 80px;
}

.list-item {
    position: relative;
    display: -ms-flexbox;
    display: flex;
    display: -webkit-box;
    box-sizing: border-box;
    padding: 11px 12px;
    width: 100%;
    height: 80px;
    text-decoration: none;
    border-bottom: 1px solid #f7f7f7;
    color: #000;
}

.listitem-box button.getuserinfobtn {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background-color: white;
    border: none;
    margin: 0;
    padding: 0;
    z-index: 10000;
    opacity: 0;
}

.cover {
    position: relative;
    z-index: 1;
    display: inline-block;
    margin: 0 auto;
    text-align: center;
    box-shadow: 0 1px 1px 1px rgba(0,0,0,0.25);
    overflow: hidden;
    background: url(http://quce.cdn.woquhudong.cn/index/img/game-icon.png) no-repeat;
    margin-right: 10px;
    margin-left: 2px;
    background-size: 60px 60px;
    background-position: center;
    background-color: rgba(241,241,241,0.5);
    -webkit-box-flex: 1;
}

.cover,.cover image {
    border-radius: 12px;
    width: 60px;
    height: 60px;
}

.diy-meta {
    -webkit-box-flex: 1;
    flex: 1;
    color: #9b9b9b;
    overflow: hidden;
    height: 60px;
}

.diy-meta .title {
    color: #333;
    font-weight: 400;
    font-size: 14px;
    margin-bottom: 2px;
    margin-top: -2px;
}

.diy-meta .count {
    font-size: 12px;
    margin-right: 4px;
    margin-left: 4px;
}

.diy-meta .desc {
    line-height: 1;
    font-size: 12px;
    margin-top: 5px;
    padding-right: 50px;
}

.mui-ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.search .form {
    display: flex;
    box-sizing: border-box;
    position: relative;
    height: 45px;
    padding-top: 8px;
    background: #fff;
    flex-direction: row;
    width: 100%;
}

.search .form input {
    width: 100%;
    height: 100%;
    padding: 0 10px;
    box-sizing: border-box;
    color: #999;
    font-size: 14px;
}

.search .search-input {
    display: -webkit-box;
    -webkit-box-flex: 1;
    flex: 1;
    box-sizing: border-box;
    z-index: 6;
    padding: 0;
    margin: 0 0.625rem;
    height: 30px;
    border: 1px solid #e5e5e5;
    border-radius: 0.25rem;
}

.search .form button {
    box-sizing: content-box;
    width: 50px;
    height: 30px;
    margin: 0 0.625rem 0 0;
    padding: 0;
    border: 0;
    background: #ff7585;
    color: white;
    -webkit-box-flex: 1;
    display: inline-block;
    position: relative;
}

.diy-more {
    float: right;
    text-align: right;
    text-decoration: none;
    color: #5b5b5b;
    font-family: "microsoft yahei",sans-serif;
    font-size: 12px;
    font-weight: 400;
    line-height: 1;
}

.playbtn {
    position: absolute;
    display: block;
    width: auto;
    color: #ff7585;
    min-width: 30px;
    right: 10px;
    top: 30px;
    padding: 0 6px;
    border: 1px solid;
    border-radius: 3px;
    text-decoration: none;
    font-family: "microsoft yahei",sans-serif;
    font-size: 12px;
}

.home_daily_box {
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background-color: rgba(0,0,0,0.7);
    z-index: 99999;
}

.home_daily_box_newquiz {
    position: relative;
    width: 89%;
    max-width: 569px;
    margin: 90px auto 0 auto;
    border-radius: 10px;
    background-color: #fff;
    text-align: center;
    z-index: 99999;
    color: #ff7585;
    font-family: 'microsoft yahei',sans-serif;
    overflow: hidden;
}

.home_daily_box_title {
    position: relative;
    display: block;
    width: 100%;
    height: 42px;
    line-height: 42px;
    font-size: 18px;
    margin: 0 auto;
    overflow: hidden;
}

.home_daily_box_title_lefttip {
    display: block;
    position: absolute;
    height: 18px;
    line-height: 18px;
    left: -22px;
    font-size: 12px;
    top: 5px;
    color: #fff;
    background-color: #ff7585;
    padding: 0 25px;
    transform-origin: center center;
    transform: rotate(-45deg);
}

.home_daily_box_closebtn {
    position: absolute;
    width: 24px;
    height: 24px;
    right: 5px;
    top: 5px;
    display: block;
    line-height: 22px;
    font-size: 12px;
    font-weight: 700;
}

.home_daily_box_closebtn_image {
    position: absolute;
    display: block;
    width: 24px;
    height: 24px;
    right: 5px;
    top: 5px;
}

.home_daily_box_list {
    width: 90%;
    margin: 0 auto;
}

.home_daily_box_list_item {
    display: block;
    width: 100%;
    height: 35px;
    line-height: 35px;
    font-size: 15px!important;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    text-align: left;
    position: relative;
}

.home_daily_box_list_item:first-child::before {
    content: "";
    width: 100%;
    display: block;
    height: 1px;
    position: absolute;
    left: 0;
    top: 0;
    transform: scaleY(0.5);
    border-top: 1px dashed #ff7585;
}

.home_daily_box_list_item:after {
    content: "";
    width: 100%;
    display: block;
    height: 1px;
    position: absolute;
    left: 0;
    bottom: 0;
    transform: scaleY(0.5);
    border-bottom: 1px dashed #ff7585;
}

.home_daily_box_item_dot {
    width: 5px;
    height: 5px;
    background-color: #ff7585;
    display: inline-block;
    border-radius: 50%;
    margin-right: 5px;
    margin-bottom: 2px;
}

.home_daily_box_item_title {
    display: inline;
    max-width: 85%;
    overflow: hidden;
    text-overflow: ellipsis;
    color: #ff7585;
    text-decoration: none;
    font-size: 15px;
    line-height: 15px;
}

.home_daily_box_item_label {
    height: 35px;
    line-height: 16px;
    position: relative;
    vertical-align: text-top;
    box-sizing: border-box;
    display: none;
}

.home_daily_box_item_label_txt {
    display: inline;
    background-color: #ff7585;
    color: white;
    font-size: 10px;
    border-radius: 5px;
    line-height: 14px;
    padding: 1.5px 4px 0.5px 4px;
    margin-left: 5px;
}

.home_daily_box_morebtn {
    font-size: 15px;
    display: inline-block;
    background-color: #ff7585;
    border-radius: 10px;
    margin: 20px;
    letter-spacing: 1px;
    color: #fff;
    width: 120px;
    height: 38px;
    line-height: 38px;
}

.home_daily_item3,.home_daily_item4 {
    display: inline-block;
}

.kfbtn {
    position: fixed;
    display: block;
    width: 115rpx;
    height: 115rpx;
    right: 5px;
    bottom: 53px;
}

.kfbtn button {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    opacity: 0;
    margin: 0;
    padding: 0;
}

.index_nav_box {
    position: fixed;
    display: flex;
    width: 100%;
    height: 48px;
    left: 0;
    z-index: 100;
    border-color: rgba(0,0,0,0.33);
    color: rgb(95,95,95);
    bottom: 0;
    background-color: #f7f7fa;
}

.index_nav_splite {
    border-color: rgba(0,0,0,0.33);
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 1px;
    background-color: #c0bfc4;
    transform: scaleY(0.5);
}

.index_nav_homebtn,.index_nav_minebtn {
    display: block;
    flex: 1;
    padding: 5px 0 0;
    font-size: 0;
    text-align: center;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    color: rgb(219,73,112);
    box-sizing: border-box;
}

.index_nav_imagebox {
    display: inline-block;
    position: relative;
}

.index_nav_imagebox image {
    display: inline-block;
    width: 27px;
    height: 27px;
}

.index_nav_imagebox_view {
    position: absolute;
    top: 0px;
    right: -6px;
    display: none;
}

.index_nav_txt {
    text-align: center;
    color: #999;
    font-size: 10px;
    line-height: 1;
}

.index_nav_txt_index {
    color: rgb(219,73,112);
}

.home_bottom_nav_novel_reddot {
    position: absolute;
    display: none;
    right: 50%;
    top: 2px;
    font-size: 10px;
    background: red;
    color: white;
    width: 16px;
    height: 16px;
    line-height: 16px;
    border-radius: 50%;
    transform: translateX(120%);
}

.float_kefu_button {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 100;
}

.float_kefu_button button {
    margin: 0;
    padding: 0;
    border: none;
    width: 100%;
    height: 100%;
    opacity: 0;
}

.attention_container {
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 100;
    display: none;
}

.attention_floatbg {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background-color: rgba(0,0,0,0.4);
}

.attention_alertbox {
    position: relative;
    width: 82%;
    height: 180px;
    margin: 0 auto;
    top: 42%;
    transform: translateY(-50%);
    border-radius: 5px;
    background-color: white;
    padding-top: 20px;
    box-sizing: border-box;
}

.attention_btn_close {
    position: absolute;
    right: 5px;
    top: 5px;
    width: 24px;
    height: 24px;
    display: block;
}

.attention_title {
    display: block;
    width: 100%;
    height: 18px;
    line-height: 18px;
    text-align: center;
}

.attention_title_span {
    display: inline-block;
    height: 18px;
    font-size: 18px;
    color: #575757;
    margin: 0 5px;
}

.attention_title_img {
    display: inline-block;
    width: 19px;
    margin-bottom: -2px;
}

.attention_quiztitle {
    display: block;
    width: 100%;
    height: 20px;
    margin-top: 34px;
    font-size: 18px;
    color: #ff7585;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 20px;
}

.attention_btn_play {
    display: block;
    margin-top: 34px;
    height: 34px;
    text-align: center;
}

.attention_btn_play_span {
    display: inline-block;
    width: 108px;
    height: 34px;
    font-size: 18px;
    line-height: 34px;
    background-color: #ff7585;
    color: white;
    border-radius: 10px;
}