<view class="result">
  <image class="pic" src="{{url}}"></image>
  <view class="btn_box">
    <button class="share" hoverClass="none" openType="share">
      <image src="../assets/share.png"></image>
    </button>
    <button bindtap="save" class="save" hoverClass="none" openType="{{openType}}">
      <image src="../assets/save.png"></image>
    </button>
  </view>
  <image bindtap="back" class="again" src="../assets/again.png"></image>
