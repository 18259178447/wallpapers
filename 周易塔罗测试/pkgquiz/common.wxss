    height: 1px;
    background-color: #c0bfc4;
    transform: scaleY(0.5);
}

.index_nav_homebtn,.index_nav_minebtn {
    display: block;
    flex: 1;
    padding: 5px 0 0;
    font-size: 0;
    text-align: center;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    color: rgb(219,73,112);
