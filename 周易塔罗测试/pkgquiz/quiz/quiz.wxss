
@import "../common.wxss";

.full_wrapper {
    margin: auto;
    max-width: 640px;
    height: 100%;
    display: -webkit-box;
    display: -ms-flexbox;
    -webkit-box-orient: vertical;
    -moz-box-orient: vertical;
    overflow: hidden;
}

.main {
    flex: 1;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    height: 100%;
    display: -webkit-box;
    display: -ms-flexbox;
    -webkit-box-orient: vertical;
    -moz-box-orient: vertical;
    -webkit-overflow-scrolling: touch;
}

.load_mask {
    text-align: center;
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    z-index: 99;
    background-color: #ebebeb;
    display: table;
}

.loading_item_container {
    display: table-cell;
    vertical-align: middle;
}

.loading_hint {
    display: inline-block;
    color: #5e5e5e;
    font-size: 20px;
    padding-left: 10px;
    vertical-align: middle;
}

.loading_gif {
    width: 30px;
    height: 33px;
    display: inline-block;
    vertical-align: middle;
}

.xb_pageCont {
    flex: 1;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    max-width: 640px;
    margin: auto;
    width: 100%;
    background-color: #e5e5ea;
}

.xb_flex {
    display: -webkit-box;
    display: -ms-flexbox;
    -webkit-box-orient: vertical;
    -moz-box-orient: vertical;
    overflow: hidden;
}

.page_body {
    flex: 1;
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    height: auto;
    background: #e5e5ea;
    overflow-y: auto;
}

.xb_head_padding {
    padding-top: 0;
}

.xb_conversation_item {
    padding: 8px 10px;
    margin: 0;
}

.xb_conv_wrap_left,.xb_conv_wrap_right {
    clear: both;
    overflow: hidden;
    position: relative;
}

.xb_conv_left,.xb_conv_right {
    border-radius: 5px;
    -khtml-border-radius: 5px;
    padding: 9px;
    font-size: 15px;
    line-height: 20px;
    color: #333;
    min-height: 20px;
    display: inline-block;
    word-break: break-all;
    word-wrap: break-word;
}

.xb_tri_left,.xb_tri_left1 {
    border-top: 6px solid transparent;
    border-bottom: 6px solid transparent;
    margin: 14px 0;
    float: left;
}

.xb_avatar_left {
    float: left;
    width: 40px;
    height: 40px;
    background-size: cover;
}

.xb_tri_left {
    border-right: 6px solid #fff;
    left: 7px;
    z-index: 1;
    position: relative;
}

.xb_tri_left1 {
    border-right: 6px solid #d0d0d0;
}

.xb_conv_left {
    max-width: 215px;
    margin-left: 0;
    margin-right: auto;
    background-color: #fff;
    border: 1px solid #d0d0d0;
    overflow: hidden;
    position: relative;
}

.xb_tri_right,.xb_tri_right1 {
    border-top: 6px solid transparent;
    border-bottom: 6px solid transparent;
    float: right;
    margin: 14px 0;
}

.xb_avatar_right {
    float: right;
    width: 40px;
    height: 40px;
    background-image: url(http://quce.cdn.woquhudong.cn/entry/img/v8/user_avatar.png);
    background-size: cover;
}

.xb_tri_right {
    border-left: 6px solid #a0e75a;
    right: 7px;
    z-index: 1;
    position: relative;
}

.xb_tri_right1 {
    border-left: 6px solid #83d45a;
}

.xb_conv_right {
    max-width: 180px;
    margin-right: 0;
    margin-left: auto;
    color: #333;
    float: right;
    background-color: #a0e75a;
    border: 1px solid #83d45a;
}

.img-longtap-fix {
    position: relative;
}

.xb_sendimg {
    display: block;
    min-width: 150px;
    min-height: 75px;
    max-width: 100%;
    background: url(http://quce.cdn.woquhudong.cn/entry/img/v8/Loading.gif) no-repeat center;
}

.resultimage {
    margin-bottom: -50px!important;
}

.xb_sendimg_opcity {
    position: absolute;
    display: block;
    width: 215px;
    left: 9px;
    top: 9px;
    padding-bottom: 7px;
    opacity: 0;
}

.xb_sendimg_qrcode {
    position: absolute;
    width: 215px;
    height: 60px;
    left: 9px;
    bottom: 0px;
    display: none;
}

.xb_btn_area {
    z-index: 10;
    width: 100%;
    display: table;
    background: #f4f4f6;
    table-layout: fixed;
    border-top: #dcdcdc solid 1px;
}

.xb_btn_container {
    display: table-cell;
    padding: 10px 5px;
    overflow: hidden;
    white-space: nowrap;
    min-width: 80px;
}

.btn_basic {
    height: 45px;
    line-height: 45px;
    font-size: 17px;
    text-align: center;
    color: #000;
    display: block;
    text-decoration: none;
}

.wechatGreen {
    background: #a0e75a;
    border: #83d45a solid 1px;
    border-radius: 5px;
    color: #333;
}

.btn_basic {
    border: none!important;
    margin: 0;
}

.disabled {
    background: #aaa;
    border-color: #aaa;
    color: #fff;
}

.scrollx {
    display: block;
    overflow-x: auto;
}

.savetipsbox {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 100000;
}

.savetips {
    width: 110px;
    height: 110px;
    position: absolute;
    left: 50%;
    top: 40%;
    transform: translate(-50%,-50%);
}

.fixbox_container {
    position: fixed;
    display: none;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 1000;
    background-color: rgba(0,0,0,0.6);
}

.authorize_box {
    display: block;
    position: absolute;
    width: 624rpx;
    min-height: 626rpx;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    background-color: white;
    border-radius: 10rpx;
    text-align: center;
    overflow: hidden;
}

.authorize_title {
    color: black;
    font-size: 18px;
    height: 90rpx;
    line-height: 90rpx;
}

.authorize_splitline {
    display: block;
    height: 1rpx;
    width: 100%;
    background-color: #dcdadd;
}

.authorize_imagebox {
    width: 100%;
    padding: 32rpx 0 0 0;
    box-sizing: border-box;
    text-align: left;
}

.authorize_text1 {
    color: #888;
    width: 100%;
    padding: 0 58rpx 0 52rpx;
    box-sizing: border-box;
    font-size: 14px;
}

.authorize_text2 {
    color: #ff4719;
    margin-bottom: 20rpx;
    width: 100%;
    padding: 0 40rpx 0 52rpx;
    box-sizing: border-box;
    font-size: 16px;
}

.authorize_text3 {
    width: 100%;
    padding: 0 52rpx;
    box-sizing: border-box;
    margin-bottom: 10rpx;
}

.authorize_text3 image {
    width: 100%;
}

.authorize_btn {
    height: 80rpx;
    line-height: 80rpx;
    color: #0a0;
    font-size: 18px;
}

.audio_image_box {
    display: block;
    height: 20px;
    line-height: 20px;
    position: relative;
    overflow: hidden;
    width: 100px;
}

.audio_image_box image {
    position: absolute;
    width: 10px;
    top: 3px;
}

.audio_image_ani {
    display: none;
}

.audio_image_play .audio_image_ani {
    display: block;
}

.reddot {
    position: absolute;
    display: block;
    width: 8px;
    height: 8px;
    border-radius: 50%;
    background-color: red;
    left: 176px;
    top: 1px;
}

.imageArr_item {
    position: relative;
    width: 105px;
    display: inline-block;
    overflow: hidden;
}

.imageArr_item:nth-child(2n) {
    margin-left: 5px;
}

.imageArr_item_image {
    display: block;
    min-width: 107.5px;
    min-height: 75px;
    max-width: 100%;
    background: url(http://quce.cdn.woquhudong.cn/entry/img/v8/Loading.gif) no-repeat center;
}

.imageArr_item_desc {
    position: absolute;
    width: 30px;
    height: 30px;
    line-height: 30px;
    left: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.6);
    color: white;
    display: inline-block;
    font-size: 14px;
    text-align: center;
}

.question_indexes_box {
    display: block;
    padding: 8px 0;
    text-align: center;
}

.question_indexes {
    text-align: center;
    color: white;
    background-color: #cecece;
    display: inline-block;
    margin: 0 auto;
    height: 21px;
    line-height: 21px;
    font-size: 12px;
    border-radius: 3px;
    padding: 0 5px;
}

.question_options_box {
    padding: 0px 10px 8px 52px;
    margin: 0;
}

.question_options_container {
    width: 79%;
    margin-left: 0;
    margin-right: auto;
    background-color: #fff;
    border: 1px solid #d0d0d0;
    position: relative;
    padding: 9px;
    border-radius: 5px;
}

.options_question_item {
    display: block;
    margin-bottom: 7px;
    line-height: 24px;
    border: 1px solid #d0d0d0;
    padding: 6px 25px 6px 8px;
    border-radius: 5px;
    font-size: 16px;
    position: relative;
}

.options_question_item_select {
    border: 1px solid #fc0;
    background-color: #fc0;
}

.options_question_icon {
    display: block;
    position: absolute;
    width: 18px;
    top: 50%;
    right: 4px;
    transform: translateY(-50%);
}

.options_question_select {
    display: none;
}

.options_question_item_select .options_question_uncheck {
    display: none;
}

.options_question_item_select .options_question_select {
    display: block;
}

.options_question_item:last-child {
    margin-bottom: 0;
}

.option_image_item {
    width: calc(50% - 4.5px);
    display: inline-block;
    overflow: hidden;
    position: relative;
    background-position: center;
    background-size: cover;
}

.question_options_image_box {
    margin-bottom: 8px;
}

.option_image_item:nth-child(2n) {
    margin-left: 9px;
}

.options_key {
    position: absolute;
    width: 30px;
    height: 30px;
    line-height: 30px;
    left: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.6);
    color: white;
    display: inline-block;
    font-size: 14px;
    text-align: center;
}

.new_template_help {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 10;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.8);
    display: none;
}

.helper_bg {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
}

.new_template_help image {
    display: block;
    width: 70%;
    left: 15%;
    bottom: 10%;
    position: absolute;
}