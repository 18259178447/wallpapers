.pagebox {
    position: relative;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.list {
    display: flex;
    flex-flow: row;
    height: 100rpx;
    background-color: #FFF;
    border-bottom: 1rpx solid #EEEEEE;
    padding: 15rpx 20rpx 0 20rpx;
}

.tbox {
    display: flex;
    flex-flow: column;
    flex: 1;
}

.title {
    line-height: 50rpx;
    font-size: 32rpx;
}

.times {
    color: #919191;
    font-size: 25rpx;
}

.jine {
    font-size: 35rpx;
    color: #00AE7D;
    line-height: 90rpx;
}

.tmons {
    color: #ff0000;
}